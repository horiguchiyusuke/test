package while文;

public class while文基礎 {

	public static void main(String[] args) {
		// TODO 自動生成されたメソッド・スタブ

int i=1;
while(i<=5){
	System.out.println(i);
i++;
}

int i1=0;
while(i1<10){
	System.out.println(i1);
	i1++;
}
		
int i2=5;
while(i2<10){
System.out.println(i2);
i2++;
}

int i3=5;
while(i3>0)
{
System.out.println(i3);
i3--;
}		

int i4=10;
while(i4>=5){
System.out.println(i4);
i4--;
}
		
		
		
		
		
		
		
		
		
		
		
		
		
	}

}
