package while文;

public class while文応用 {

	public static void main(String[] args) {
		// TODO 自動生成されたメソッド・スタブ

int i=1;
do{
System.out.println(i);
i--;
}while(i>1);


int i1=0;
do{
System.out.println(i1);
i1++;
}while(i1<10);
		
int i2=5;
do{
	System.out.println(i2);
i2++;
}while(i2<10);

int i3=5;
do{
System.out.println(i3);
i3--;
}while(i3>0);
	}

}
