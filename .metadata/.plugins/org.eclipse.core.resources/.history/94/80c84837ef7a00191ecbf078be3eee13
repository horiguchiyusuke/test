package 配列;

public class 多次元配列 {

	public static void main(String[] args) {
		// TODO 自動生成されたメソッド・スタブ

String[][]country=
{
{"日本","タイ"},
{"アメリカ","ブラジル"},
{"フランス","ロシア"}
};
System.out.println(country[0][0]);

String[][]country1=
{
{"日本","タイ"},
{"アメリカ","ブラジル"},
{"フランス","ロシア"}
};
System.out.println(country1[0][1]);

String[][]country2=
{
{"日本","タイ"},
{"アメリカ","ブラジル"},
{"フランス","ロシア"}
};
System.out.println(country2[1][0]);

String[][]country3=
{
{"日本","タイ"},
{"アメリカ","ブラジル"},
{"フランス","ロシア"}
};
System.out.println(country3[2][1]);

String[][] country5= new String[3][2];
country5[0][0]="日本";
country5[0][1]="タイ";
country5[1][0]="アメリカ";
country5[1][1]="ブラジル";
country5[2][0]="フランス";
country5[2][1]="ロシア";
System.out.println(country5[0][0]);
	}

	}

